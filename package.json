{
  "name": "mkn-ui",
  "version": "1.0.0",
  "description": "MunchKiN server and client connecting to mkn-api",
  "main": "/server/index.ts",
  "scripts": {
    "dev": "concurrently --kill-others \"yarn watch-client\" \"yarn watch-server\"",
    "watch-server": "nodemon --watch 'server/**/*' -e ts,tsx --exec ts-node ./server/index.ts",
    "watch-client": "webpack --watch --config webpack.config.js",
    "build-client:dev": "webpack --config webpack.config.js",
    "test-e2e": "cypress run --headed --no-exit"
  },
  "keywords": [
    "MunchKiN"
  ],
  "author": "Calvin Lee",
  "license": "ISC",
  "repository": "https://github.com/clee421/mkn-ui",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@csstools/normalize.css": "^10.1.0",
    "apollo-boost": "^0.4.4",
    "apollo-server-koa": "^2.9.7",
    "axios": "^0.19.0",
    "graphql": "^14.5.8",
    "koa": "^2.8.2",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "pug": "^3.0.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/koa": "^2.0.50",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa-views": "^2.0.3",
    "@types/pug": "^2.0.4",
    "@types/react": "^16.9.3",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router-dom": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "react-redux": "^7.1.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
